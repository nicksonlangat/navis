{"ast":null,"code":"import { TransitionRoot, TransitionChild, Dialog, DialogPanel, DialogTitle } from '@headlessui/vue';\nimport { mapActions } from 'vuex';\nexport default {\n  components: {\n    TransitionRoot,\n    TransitionChild,\n    Dialog,\n    DialogPanel,\n    DialogTitle\n  },\n  data() {\n    return {\n      isOpen: false,\n      truck: {\n        manufacturer: \"\",\n        model: \"\",\n        chasis_number: \"\",\n        registration_number: \"\",\n        engine_size: \"\",\n        engine_power: \"\",\n        yom: \"\"\n      }\n    };\n  },\n  methods: {\n    ...mapActions({\n      updateTruck: 'updateTruck',\n      deleteTruck: 'deleteTruck'\n    }),\n    toggleModal() {\n      this.isOpen = !this.isOpen;\n    },\n    updateTruckDetails() {\n      this.updateTruck({\n        uuid: this.truck.id,\n        payload: this.truck,\n        cb: res => {\n          this.toggleModal();\n          this.emitter.emit(\"reloadTrucks\");\n        }\n      });\n    },\n    deleteTruckDetails() {\n      this.deleteTruck({\n        uuid: this.truck.id,\n        cb: res => {\n          this.toggleModal();\n          this.emitter.emit(\"reloadTrucks\");\n        }\n      });\n    }\n  },\n  mounted() {\n    this.emitter.on(\"showTruckModal\", value => {\n      this.truck = value;\n      this.toggleModal();\n    });\n  }\n};","map":{"version":3,"names":["TransitionRoot","TransitionChild","Dialog","DialogPanel","DialogTitle","mapActions","components","data","isOpen","truck","manufacturer","model","chasis_number","registration_number","engine_size","engine_power","yom","methods","updateTruck","deleteTruck","toggleModal","updateTruckDetails","uuid","id","payload","cb","res","emitter","emit","deleteTruckDetails","mounted","on","value"],"sources":["/Users/nicklangat/Desktop/portfolio/logistics/src/components/EditTruckModal.vue"],"sourcesContent":["<template>\n<TransitionRoot appear :show=\"isOpen\" as=\"template\">\n    <Dialog as=\"div\" @close=\"toggleModal\" class=