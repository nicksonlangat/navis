{"ast":null,"code":"import { TransitionRoot, TransitionChild, Dialog, DialogPanel, DialogTitle } from '@headlessui/vue';\nimport { mapActions, mapGetters } from 'vuex';\nexport default {\n  components: {\n    TransitionRoot,\n    TransitionChild,\n    Dialog,\n    DialogPanel,\n    DialogTitle\n  },\n  data() {\n    return {\n      isOpen: false,\n      shipment: {\n        truck: \"\",\n        driver: \"\",\n        destination: \"\",\n        departure_date: \"\",\n        arrival_date: \"\",\n        status: \"\"\n      }\n    };\n  },\n  computed: {\n    ...mapGetters({\n      storedTrucks: 'getStoredTrucks',\n      storedDrivers: 'getStoredDrivers'\n    }),\n    trucks() {\n      return this.storedTrucks;\n    },\n    drivers() {\n      return this.storedDrivers;\n    }\n  },\n  methods: {\n    ...mapActions({\n      createShipment: 'createShipment',\n      getAllTrucks: 'getAllTrucks',\n      getAllDrivers: 'getAllDrivers'\n    }),\n    submitNewShipment() {\n      this.createShipment({\n        payload: this.shipment,\n        cb: res => {\n          this.closeModal();\n          this.emitter.emit(\"reloadShipments\");\n        }\n      });\n    },\n    closeModal() {\n      this.isOpen = false;\n    },\n    openModal() {\n      this.isOpen = true;\n    }\n  },\n  mounted() {\n    this.getAllTrucks({\n      cb: res => {}\n    });\n    this.getAllDrivers({\n      cb: res => {}\n    });\n  }\n};","map":{"version":3,"names":["TransitionRoot","TransitionChild","Dialog","DialogPanel","DialogTitle","mapActions","mapGetters","components","data","isOpen","shipment","truck","driver","destination","departure_date","arrival_date","status","computed","storedTrucks","storedDrivers","trucks","drivers","methods","createShipment","getAllTrucks","getAllDrivers","submitNewShipment","payload","cb","res","closeModal","emitter","emit","openModal","mounted"],"sources":["/Users/nicklangat/Desktop/portfolio/logistics/src/components/NewShipmentDrawer.vue"],"sourcesContent":["<templa