{"ast":null,"code":"import { TransitionRoot, TransitionChild, Dialog, DialogPanel, DialogTitle } from '@headlessui/vue';\nimport { mapActions, mapGetters } from 'vuex';\nexport default {\n  components: {\n    TransitionRoot,\n    TransitionChild,\n    Dialog,\n    DialogPanel,\n    DialogTitle\n  },\n  data() {\n    return {\n      isOpen: false,\n      truck: {\n        manufacturer: \"\",\n        model: \"\",\n        chasis_number: \"\",\n        registration_number: \"\",\n        engine_size: \"\",\n        engine_power: \"\",\n        yom: \"\"\n      }\n    };\n  },\n  computed: {\n    ...mapGetters({\n      storedClients: 'getStoredClients'\n    }),\n    clients() {\n      return this.storedClients;\n    }\n  },\n  methods: {\n    ...mapActions({\n      updateParcel: 'updateParcel',\n      deleteParcel: 'deleteParcel',\n      getAllClients: 'getAllClients'\n    }),\n    toggleModal() {\n      this.isOpen = !this.isOpen;\n    },\n    updateParcelDetails() {\n      this.updateParcel({\n        uuid: this.parcel.id,\n        payload: this.parcel,\n        cb: res => {\n          this.toggleModal();\n          this.emitter.emit(\"reloadParcels\");\n        }\n      });\n    },\n    deleteParcelDetails() {\n      this.deleteParcel({\n        uuid: this.parcel.id,\n        cb: res => {\n          this.toggleModal();\n          this.emitter.emit(\"reloadParcels\");\n        }\n      });\n    }\n  },\n  mounted() {\n    this.emitter.on(\"showTruckModal\", value => {\n      this.truck = value;\n      this.toggleModal();\n    });\n  }\n};","map":{"version":3,"names":["TransitionRoot","TransitionChild","Dialog","DialogPanel","DialogTitle","mapActions","mapGetters","components","data","isOpen","truck","manufacturer","model","chasis_number","registration_number","engine_size","engine_power","yom","computed","storedClients","clients","methods","updateParcel","deleteParcel","getAllClients","toggleModal","updateParcelDetails","uuid","parcel","id","payload","cb","res","emit