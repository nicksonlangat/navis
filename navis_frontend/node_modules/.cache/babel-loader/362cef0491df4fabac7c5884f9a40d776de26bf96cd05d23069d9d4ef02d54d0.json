sis_number: \"\",\n                registration_number: \"\",\n                engine_size: \"\",\n                engine_power: \"\",\n                yom: \"\",\n            }\n\n        }\n    },\n    computed: {\n        ...mapGetters({\n            storedClients: 'getStoredClients'\n        }),\n        clients() {\n            return this.storedClients\n        }\n    },\n    methods: {\n        ...mapActions({\n            updateParcel: 'updateParcel',\n            deleteParcel: 'deleteParcel',\n            getAllClients: 'getAllClients',\n        }),\n        toggleModal() {\n            this.isOpen = !this.isOpen\n        },\n        updateParcelDetails() {\n            this.updateParcel({\n                uuid: this.parcel.id,\n                payload: this.parcel,\n                cb: (res => {\n                    this.toggleModal()\n                    this.emitter.emit(\"reloadParcels\")\n                })\n            })\n        },\n        deleteParcelDetails() {\n            this.deleteParcel({\n                uuid: this.parcel.id,\n                cb: (res => {\n                    this.toggleModal()\n                    this.emitter.emit(\"reloadParcels\")\n                })\n            })\n        }\n    },\n    mounted() {\n        this.emitter.on(\"showTruckModal\", value => {\n            this.truck = value\n            this.toggleModal()\n        })\n    }\n}\n</script>\n"],"mappings":"AAgEA,SACIA,cAAc,EACdC,eAAe,EACfC,MAAM,EACNC,WAAW,EACXC,WAAW,QACR,iBAAgB;AACvB,SACIC,UAAU,EACVC,UAAS,QACN,MAAK;AAEZ,eAAe;EACXC,UAAU,EAAE;IACRP,cAAc;IACdC,eAAe;IACfC,MAAM;IACNC,WAAW;IACXC;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,MAAM,EAAE,KAAK;MACbC,KAAK,EAAE;QACHC,YAAY,EAAE,EAAE;QAChBC,KAAK,EAAE,EAAE;QACTC,aAAa,EAAE,EAAE;QACjBC,mBAAmB,EAAE,EAAE;QACvBC,WAAW,EAAE,EAAE;QACfC,YAAY,EAAE,EAAE;QAChBC,GAAG,EAAE;MACT;IAEJ;EACJ,CAAC;EACDC,QAAQ,EAAE;IACN,GAAGZ,UAAU,CAAC;MACVa,aAAa,EAAE;IACnB,CAAC,CAAC;IACFC,OAAOA,CAAA,EAAG;MACN,OAAO,IAAI,CAACD,aAAY;IAC5B;EACJ,